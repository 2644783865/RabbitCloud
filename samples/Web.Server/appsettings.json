{
  "ServiceHost": "localhost",
  "ConsulHost": "localhost",
  "ServicePort": 5000,
  "RabbitCloud": {
    "Consul": {
      "Address": "http://${ConsulHost}:8500/",
      "Discovery": {
        "InstanceId": "UserService_localhost",
        "ServiceName": "UserService",
        "HostName": "${ServiceHost}",
        "Port": "${ServicePort}",
        "HealthChecks": [
          {
            "Url": "http://${ServiceHost}:${ServicePort}/api/User/1",
            "Interval": "10s"
          }
        ],
        "HealthCheckUrl": "http://${ServiceHost}:${ServicePort}/health",
        "HealthCheckInterval": "10s",
        "Tags": [
          "samples"
        ]
      }
    }
  }
}