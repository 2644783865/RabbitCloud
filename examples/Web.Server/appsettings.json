{
  "Environments": {
    "ServiceHost": "192.168.1.100",
    "ConsulHost": "192.168.1.100",
    "ServicePort": 5000
  },
  "RabbitCloud": {
    "Consul": {
      "Address": "http://${Environments:ConsulHost}:8500/",
      "Discovery": {
        "InstanceId": "UserService_localhost",
        "ServiceName": "UserService",
        "HostName": "${Environments:ServiceHost}",
        "Port": "${Environments:ServicePort}",
        "HealthCheckUrl": "http://${Environments:ServiceHost}:${Environments:ServicePort}/health",
        "HealthCheckInterval": "10s",
        "Tags": [
          "examples"
        ]
      }
    }
  }
}